
filetype plugin on
syntax on
set nowrap
set laststatus=2
set undolevels=1000
imap <C-q> <esc>:q<cr>i
nmap <C-q> :q<cr

nmap <F5> :q<CR>
imap <F5> <Esc>:q<CR>
vmap <F5> <Esc>:q<CR>

    nmap <F5> :q<CR>
    imap <F5> <Esc>:q<CR>
    vmap <F5> <Esc>:q<CR>





nmap <F2> :w<cr>
vmap <F2> <esc>:w<cr>i
imap <F2> <esc>:w<cr>i
set showmode
